<div class="about-page twcss-relative md:twcss-top-0 -!twcss-top-16 md:twcss-pt-[256px] content-black">
  {% for block in section.blocks %}
    <div class="about-block so-page-margins twcss-mb-[32px] md:twcss-mb-[64px]">
      <!-- Block type 1: 1 image, 1 title, 1 rich text -->
      {% if block.type == 'one_img_title' %}
        <div class="single-image-title-text-block twcss-relative">
          {% if block.settings.direction == 'text_first' %}
            <div class="text-first so-grid twcss-gap-y-[32px] ">
              <div class="twcss-col-span-6 about-text-container twcss-relative">
                <h2 class="about-title-text">{{ block.settings.title }}</h2>
                <div class="rich-text goudy">{{ block.settings.rich_text }}</div>
                {% if block.settings.illustration_1 %}
                  <img src="{{ block.settings.illustration_1 | image_url }}" class="illustration {{ block.settings.illustration_position_1 }} " style="width:{{ block.settings.illustration_width_1 }}%; transform:translate({{ block.settings.illustration_translate_x_1 }}%, {{ block.settings.illustration_translate_y_1 }}%); " />
                {% endif %}
              </div>
              <div class="twcss-col-span-6 twcss-relative">
                <img class="twcss-object-cover" src="{{ block.settings.image | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio }};" alt="{{ block.settings.image.alt }}" />
                {% if block.settings.illustration_2 %}
                  <img src="{{ block.settings.illustration_2 | image_url }}" class="illustration {{ block.settings.illustration_position_2 }} " style="width:{{ block.settings.illustration_width_2 }}%; transform:translate({{ block.settings.illustration_translate_x_2 }}%, {{ block.settings.illustration_translate_y_2 }}%); " />
                {% endif %}
              </div>
            </div>
          {% else %}
            <div class="image-first so-grid twcss-gap-y-[32px]">
              <div class="twcss-col-span-6 twcss-relative">
                <img class="twcss-object-cover" src="{{ block.settings.image | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio }};" alt="{{ block.settings.image.alt }}" />
                {% if block.settings.illustration_1 %}
                  <img src="{{ block.settings.illustration_1 | image_url }}" class="illustration {{ block.settings.illustration_position_1 }} " style="width:{{ block.settings.illustration_width_1 }}%; transform:translate({{ block.settings.illustration_translate_x_1 }}%, {{ block.settings.illustration_translate_y_1 }}%); " />
                {% endif %}
              </div>
              <div class="twcss-col-span-6 about-text-container twcss-relative">
                <h2 class="about-title-text">{{ block.settings.title }}</h2>
                <div class="rich-text goudy">{{ block.settings.rich_text }}</div>
                {% if block.settings.illustration_2 %}
                  <img src="{{ block.settings.illustration_2 | image_url }}" class="illustration {{ block.settings.illustration_position_2 }} " style="width:{{ block.settings.illustration_width_2 }}%; transform:translate({{ block.settings.illustration_translate_x_2 }}%, {{ block.settings.illustration_translate_y_2 }}%); " />
                {% endif %}
              </div>
            </div>
          {% endif %}
          {% if block.settings.illustration_3 %}
            <img src="{{ block.settings.illustration_3 | image_url }}" class="illustration {{ block.settings.illustration_position_3 }} " style="width:{{ block.settings.illustration_width_3 }}%; transform:translate({{ block.settings.illustration_translate_x_3 }}%, {{ block.settings.illustration_translate_y_3 }}%); " />
          {% endif %}
          {% comment %} {% if block.settings.illustration_2 %}
            <img src="{{ block.settings.illustration_2 | image_url }}" class="illustration {{ block.settings.illustration_position_2 }} " style="width:{{ block.settings.illustration_width_2 }}%; transform:translate({{ block.settings.illustration_translate_x_2 }}%, {{ block.settings.illustration_translate_y_2 }}%); " />
          {% endif %} {% endcomment %}
        </div>

      <!-- Block type 2: 2 images, 1 title, 1 rich text -->
      {% elsif block.type == 'two_img_title' %}
        <div class="two-images-title-text-block twcss-relative twcss-h-max">
          {% if block.settings.direction == 'text_first' %}
            <div class="text-first so-grid twcss-gap-y-[32px]">
              <div class="twcss-col-span-6 about-text-container twcss-relative">
                <h2 class="about-title-text">{{ block.settings.title }}</h2>
                <div class="rich-text goudy">{{ block.settings.rich_text }}</div>
                {% if block.settings.illustration_1 %}
                  <img src="{{ block.settings.illustration_1 | image_url }}" class="illustration {{ block.settings.illustration_position_1 }} " style="width:{{ block.settings.illustration_width_1 }}%; transform:translate({{ block.settings.illustration_translate_x_1 }}%, {{ block.settings.illustration_translate_y_1 }}%); " />
                {% endif %}
              </div>
              <div class="image-pair twcss-col-span-6 twcss-relative">
                <img class="twcss-object-cover twcss-col-span-6 md:twcss-col-span-4" src="{{ block.settings.image_1 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio_1 }};" alt="{{ block.settings.image_1.alt }}" />
                <img class="twcss-object-cover twcss-col-span-5 md:twcss-col-span-4" src="{{ block.settings.image_2 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio_2 }};" alt="{{ block.settings.image_2.alt }}" />
                {% if block.settings.illustration_2 %}
                  <img src="{{ block.settings.illustration_2 | image_url }}" class="illustration {{ block.settings.illustration_position_2 }} " style="width:{{ block.settings.illustration_width_2 }}%; transform:translate({{ block.settings.illustration_translate_x_2 }}%, {{ block.settings.illustration_translate_y_2 }}%); " />
                {% endif %}
              </div>
            </div>
          {% else %}
            <div class="image-first so-grid twcss-gap-y-[32px]">
              <div class="image-pair twcss-col-span-6  twcss-relative full-mobile-so-grid">
                <img class="twcss-object-cover twcss-col-span-6 md:twcss-col-span-4" src="{{ block.settings.image_1 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio_1 }};" alt="{{ block.settings.image_1.alt }}" />
                {% comment %} THIS ELEMENT BELOW IS WHAT I AM TRYING TO ABSOLUTELY POSITION {% endcomment %}
                <img class="twcss-object-cover  md:twcss-col-span-4 " src="{{ block.settings.image_2 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio_2 }};" alt="{{ block.settings.image_2.alt }}" />
                {% if block.settings.illustration_1 %}
                  <img src="{{ block.settings.illustration_1 | image_url }}" class="illustration {{ block.settings.illustration_position_1 }} " style="width:{{ block.settings.illustration_width_1 }}%; transform:translate({{ block.settings.illustration_translate_x_1 }}%, {{ block.settings.illustration_translate_y_1 }}%); " />
                {% endif %}
              </div>
              <div class="twcss-col-span-6 about-text-container twcss-relative">
                <h2 class="about-title-text">{{ block.settings.title }}</h2>
                <div class="rich-text goudy">{{ block.settings.rich_text }}</div>
                {% if block.settings.illustration_2 %}
                  <img src="{{ block.settings.illustration_2 | image_url }}" class="illustration {{ block.settings.illustration_position_2 }} " style="width:{{ block.settings.illustration_width_2 }}%; transform:translate({{ block.settings.illustration_translate_x_2 }}%, {{ block.settings.illustration_translate_y_2 }}%); " />
                {% endif %}
              </div>
            </div>
          {% endif %}
          
          {% if block.settings.illustration_3 %}
            <img src="{{ block.settings.illustration_3 | image_url }}" class="illustration {{ block.settings.illustration_position_3 }} " style="width:{{ block.settings.illustration_width_3 }}%; transform:translate({{ block.settings.illustration_translate_x_3 }}%, {{ block.settings.illustration_translate_y_3 }}%); " />
          {% endif %}
        </div>

      <!-- Block type 3: 2 images only -->
      {% elsif block.type == 'two_images' %}
      <div class="two-images-block twcss-relative so-grid twcss-relative">
        <img class="twcss-object-cover twcss-absolute twcss-w-[25%] double-image-adjustment" src="{{ block.settings.image_1 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio_1 }};" alt="{{ block.settings.image_1.alt }}" />
        <img class="twcss-object-cover twcss-col-start-4 twcss-col-end-11" src="{{ block.settings.image_2 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio_2 }};" alt="{{ block.settings.image_2.alt }}" />
        {% if block.settings.illustration_1 %}
          <img src="{{ block.settings.illustration_1 | image_url }}" class="illustration {{ block.settings.illustration_position_1 }} " style="width:{{ block.settings.illustration_width_1 }}%; transform:translate({{ block.settings.illustration_translate_x_1 }}%, {{ block.settings.illustration_translate_y_1 }}%); " />
        {% endif %}
        {% if block.settings.illustration_2 %}
          <img src="{{ block.settings.illustration_2 | image_url }}" class="illustration {{ block.settings.illustration_position_2 }} " style="width:{{ block.settings.illustration_width_2 }}%; transform:translate({{ block.settings.illustration_translate_x_2 }}%, {{ block.settings.illustration_translate_y_2 }}%); " />
        {% endif %}
        {% if block.settings.illustration_3 %}
          <img src="{{ block.settings.illustration_3 | image_url }}" class="illustration {{ block.settings.illustration_position_3 }} " style="width:{{ block.settings.illustration_width_3 }}%; transform:translate({{ block.settings.illustration_translate_x_3 }}%, {{ block.settings.illustration_translate_y_3 }}%); " />
        {% endif %}
      </div>
      {% elsif block.type == 'triple_image' %}
      <div class="three-image-grid twcss-grid twcss-grid-cols-12 twcss-pt-[32px]">
        <!-- First Image -->
        <div class="image-container twcss-col-span-4">
          <img class="twcss-object-cover" src="{{ block.settings.image_1 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio }};" alt="{{ block.settings.image_1.alt }}" />
        </div>
      
        <!-- Second Image -->
        <div class="image-container twcss-col-span-4">
          <img class="twcss-object-cover" src="{{ block.settings.image_2 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio }};" alt="{{ block.settings.image_2.alt }}" />
        </div>
      
        <!-- Third Image -->
        <div class="image-container twcss-col-span-4">
          <img class="twcss-object-cover" src="{{ block.settings.image_3 | image_url }}" style="aspect-ratio: {{ block.settings.image_aspect_ratio }};" alt="{{ block.settings.image_3.alt }}" />
        </div>
      </div>
      {% endif %}
    </div>
  {% endfor %}
  <div class="contact-area twcss-text-center twcss-py-[32px] twcss-px-[16px] md:twcss-p-[64px]">
    <h3 class="about-title-text !twcss-mb-[64px]">
      Contact
    </h3>
    <div class="twcss-flex twcss-gap-[64px] twcss-w-full md:twcss-px-[64px] md:twcss-flex-row twcss-flex-col ">
      <div class="twcss-w-full md:twcss-w-1/3 flex-center-center twcss-flex-col twcss-gap-y-[16px]">
